	<settings>
		<voice tone="en-US_AllisonV3Voice" key="1000" />
	</settings>
	<script>
		<light state="off" key="1001" />
		<evaEmotion emotion="neutral" key="1002" />
		<talk key="1003">Hi, I'm the robot Eva!</talk>
		<audio source="mario-game-intro" block="true" key="1004" />
		<talk key="1005">Shall we play the color game?</talk>
		<evaEmotion emotion="happy" key="1006" />
		<audio source="mario-start-01" block="true" key="1007" />
		<talk key="1008">What's your name?</talk>
		<audio source="mario-sound1" block="true" key="1009" />
			<led animation="listen" key="1010" />
			<wait duration="4000" key="1011" />
			<led animation="stop" key="1012" />
			<wait duration="1000" key="1013" />
		<talk key="1014">That Cool!</talk>
		<talk key="1015">Let's go Marcelo</talk>
		
		<counter var="try" op="=" value="1" key="1016" />
		<evaEmotion id="COR1" emotion="neutral" key="1017" />
		<talk key="1018">What is this color?</talk>
		<audio source="mario-sound1" block="true" key="1019" />
		<light state="on" color="blue" key="1020" />
			<led animation="listen" key="1021" />
		<wait duration="4000" key="1022" />
		<led animation="stop" key="1023" />
		<wait duration="1000" key="1024" />
			<audio source="mario-start-02" block="true" key="1025" />
			<talk key="1026">Very good! You're right!</talk>
		<evaEmotion emotion="happy" key="1027" />
		<light state="off" key="1028" />
		<wait duration="1000" key="1029" />
		
		<counter var="try" op="=" value="1" key="1030" />
		<evaEmotion id="COR2" emotion="neutral" key="1031" />
		<talk key="1032">What is this color?</talk>
		<audio source="mario-sound1" block="true" key="1033" />
		<light state="on" color="red" key="1034" />
			<led animation="listen" key="1035" />
			<wait duration="4000" key="1036" />
			<led animation="stop" key="1037" />
			<wait duration="1000" key="1038" />
				<audio source="mario-start-02" block="true" key="1039" />
				<talk key="1040">Excelent! You're right!</talk>
		<evaEmotion emotion="happy" key="1041" />
		<light state="off" key="1042" />
		<wait duration="1000" key="1043" />
		
		<counter var="try" op="=" value="1" key="1044" />
		<evaEmotion id="COR3" emotion="neutral" key="1045" />
		<talk key="1046">What is this color?</talk>
		<audio source="mario-sound1" block="true" key="1047" />
		<light state="on" color="green" key="1048" />
			<led animation="listen" key="1049" />
			<wait duration="4000" key="1050" />
			<led animation="stop" key="1051" />
			<wait duration="1000" key="1052" />
				<audio source="mario-sound1" block="true" key="1053" />
				<talk key="1054">This color is green</talk>
						<wait duration="2000" key="1055" />
						<talk key="1056">Let's try one more time</talk>
						<light state="off" key="1057" />
						<wait duration="2000" key="1058" />
					<talk key="1059">What is this color?</talk>
					<audio source="mario-sound1" block="true" key="1060" />
					<light state="on" color="green" key="1061" />
						<led animation="listen" key="1062" />
						<wait duration="4000" key="1063" />
						<led animation="stop" key="1064" />
						<wait duration="1000" key="1065" />
						<audio source="mario-start-02" block="true" key="1066" />
						<talk key="1067">Nice! You're correct!</talk>
		<evaEmotion emotion="happy" key="1068" />
		<talk key="1069">It was really nice to play with you Marcelo</talk>
		<audio source="mario-end-02" block="true" key="1070" />
		<light state="off" key="1071" />
	</script>
