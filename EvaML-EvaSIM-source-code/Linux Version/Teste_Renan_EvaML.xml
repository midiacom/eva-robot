<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Teste_Renan" xsi:noNamespaceSchemaLocation="EvaML-Schema/evaml_schema.xsd" id="9a4ab0c93200cfbd58aa398b446862f5">
  <settings>
    <voice tone="pt-BR_IsabelaV3Voice" key="1000" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <evaEmotion emotion="NEUTRAL" key="1001" />
    <light state="ON" color="WHITE" key="1002" />
    <audio source="mario-end-01" block="TRUE" key="1003" />
    <light state="ON" color="YELLOW" key="1004" />
    <evaEmotion emotion="HAPPY" key="1005" />
    <talk key="1006">Olá, eu sou o robô EVA. / 
        Olá, eu sou EVA. /
        Olá, eu sou um robô. </talk>
    <light state="ON" color="PINK" key="1007" />
    <evaEmotion emotion="NEUTRAL" key="1008" />
    <wait duration="2000" key="1009" />
    <talk key="1010">Qual é o seu nome?</talk>
    <listen key="1011" />
    <talk key="1012">Olá $, tudo bem com você? Irei te apresentar minhas emoções</talk>
    <wait duration="2000" key="1013" />
    <talk key="1014">Desse modo, estou feliz.</talk>
    <evaEmotion emotion="HAPPY" key="1015" />
    <talk key="1016">Desse outro jeito, estou triste.</talk>
    <evaEmotion emotion="SAD" key="1017" />
    <talk key="1018">E desse modo, estou com raiva.</talk>
    <evaEmotion emotion="ANGRY" key="1019" />
    <evaEmotion emotion="NEUTRAL" key="1020" />
    <talk key="1021">Você gostaria de jogar o jogo da imitação comigo?</talk>
    <listen key="1022" />
    <switch var="$"> 
      <case op="exact" value="Sim" key="1023" child_proc="true" var="$"> 
        <light state="ON" color="WHITE" key="1024" />
        <evaEmotion emotion="HAPPY" key="1025" />
        <talk key="1026">Que bom que você quer jogar comigo, vamos jogar então.</talk>
        <evaEmotion emotion="NEUTRAL" key="1027" />
        <audio source="mario-start-01" block="TRUE" key="1028" />
        <talk key="1029">Para começarmos o jogo, irei primeiro te ensinar os passos.</talk>
        <talk key="1030">Eu irei te apresentar uma das três emoções de maneira aleatória,</talk>
        <talk key="1031">Você está preparado?</talk>
        <light state="ON" color="WHITE" key="1032" />
        <evaEmotion emotion="NEUTRAL" key="1033" />
        <listen key="1034" />
        <switch var="$">
          <case op="exact" value="Sim" key="1035" child_proc="true" var="$">
            <counter var="ponto" op="=" value="0" key="1036" />
            <light state="ON" color="GREEN" key="1037" />
            <talk key="1038">Então vamos começar.</talk>
          </case>
          <case op="exact" value="Não" key="1039" child_proc="true" var="$">
            <talk key="1040">Então começaremos mais tarde.</talk>
            <stop />
          </case>
      </switch>
      </case>
      <case op="exact" value="Não" key="1041" child_proc="true" var="$">
        <light state="ON" color="RED" key="1042" />
        <evaEmotion emotion="SAD" key="1043" />
        <talk key="1044">É uma pena não jogarmos juntos, mas espero que esteja disposto</talk>
        <audio source="mario-end-03" block="TRUE" key="1045" />
      </case>
    </switch>
    <random min="0" max="2" key="1046" />
    <switch var="$">
      <case op="exact" value="0" key="1047" child_proc="true" var="$">
        <counter var="HAPPY" op="=" value="0" key="1048" />
        <evaEmotion emotion="HAPPY" key="1049" />
        <light state="ON" color="GREEN" key="1050" />      
      </case>
      <case op="exact" value="1" key="1051" child_proc="true" var="$">
        <counter var="SAD" op="=" value="1" key="1052" />
        <evaEmotion emotion="SAD" key="1053" />
        <light state="ON" color="BLUE" key="1054" />
      </case>
      <case op="exact" value="2" key="1055" child_proc="true" var="$">
        <counter var="ANGRY" op="=" value="2" key="1056" />
        <evaEmotion emotion="ANGRY" key="1057" />
        <light state="ON" color="RED" key="1058" />
      </case>
    </switch>
    <userEmotion key="1059" />
    <counter var="ponto" op="=" value="0" key="1060" />
    <random min="0" max="2" key="1061" />
    <switch var="$">
      <case op="exact" value="0" key="1062" child_proc="true" var="$">
        <evaEmotion emotion="HAPPY" key="1063" />
        <counter var="HAPPY" op="=" value="0" key="1064" />
        <light state="ON" color="GREEN" key="1065" />      
      </case>
      <case op="exact" value="1" key="1066" child_proc="true" var="$">
        <counter var="SAD" op="=" value="1" key="1067" />
        <evaEmotion emotion="SAD" key="1068" />
        <light state="ON" color="BLUE" key="1069" />
      </case>
      <case op="exact" value="2" key="1070" child_proc="true" var="$">
        <counter var="ANGRY" op="=" value="2" key="1071" />
        <evaEmotion emotion="ANGRY" key="1072" />
        <light state="ON" color="RED" key="1073" />
      </case>
    </switch>
    <userEmotion key="1074" />
    <switch var="$">
      <case op="exact" value="HAPPY" key="1075" child_proc="true" var="$">
        <counter var="ponto" op="+" value="1" key="1076" />
      </case>
      <case op="exact" value="ANGRY" key="1077" child_proc="true" var="$">
        <counter var="ponto" op="+" value="1" key="1078" />
      </case>
      <case op="exact" value="SAD" key="1079" child_proc="true" var="$">
        <counter var="ponto" op="+" value="1" key="1080" />
      </case>
    </switch>
  </script>
