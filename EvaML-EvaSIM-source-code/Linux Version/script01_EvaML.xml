<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="script01" xsi:noNamespaceSchemaLocation="EvaML-Schema/evaml_schema.xsd" id="4bc9160a149eddcf36a458f53d9cf58a">
  <settings>
    <voice tone="pt-BR_IsabelaV3Voice" key="1000" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <random min="1" max="3" key="1001" />
    <switch var="$">
      <case op="exact" value="1" key="1002" child_proc="true" var="$">
        <light state="ON" color="GREEN" key="1003" />
      </case>
      <case op="exact" value="2" key="1004" child_proc="true" var="$">
        <evaEmotion emotion="HAPPY" key="1005" />
      </case>
      <default key="1006" child_proc="true" value="" op="exact">
        <talk key="1007">Hello!</talk>
      </default>
    </switch>
  </script>
<links><link from="1000" to="1001" /><link from="1001" to="1002" /><link from="1002" to="1003" /><link from="1001" to="1004" /><link from="1004" to="1005" /><link from="1001" to="1006" /><link from="1006" to="1007" /></links></evaml>