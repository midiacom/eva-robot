<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="script01" xsi:noNamespaceSchemaLocation="EvaML-Schema/evaml_schema.xsd" id="4bc9160a149eddcf36a458f53d9cf58a">
  <settings>
  <voice tone="es-LA_SofiaV3Voice" key="1000" />
  <lightEffects mode="ON" />
  <audioEffects mode="ON" />
  </settings>
  <script>
  <light state="ON" color="WHITE" key="1001" />
  <evaEmotion emotion="NEUTRAL" key="1002" />
  <audio source="fanfare2" block="TRUE" key="1003" />
  <light state="ON" color="YELLOW" key="1004" />
  <evaEmotion emotion="HAPPY" key="1005" />
  <talk key="1006">Hola mi nombre es Eva/Hola, soy un robot social/Hola</talk>
  <light state="ON" color="PINK" key="1007" />
  <evaEmotion emotion="NEUTRAL" key="1008" />
  <wait duration="2000" key="1009" />
  <talk key="1010">Cual es tu nombre</talk>
  <listen key="1011" />
  <talk key="1012">Mucho gusto $, a continuación voy a mostrarte las emociones que puedo representar</talk>
  <talk key="1013">Felicidad</talk>
  <light state="ON" color="GREEN" key="1014" />
  <evaEmotion emotion="HAPPY" key="1015" />
  <talk key="1016">Tristesa</talk>
  <light state="ON" color="BLUE" key="1017" />
  <evaEmotion emotion="SAD" key="1018" />
  <talk key="1019">Enojo</talk>
  <light state="ON" color="RED" key="1020" />
  <evaEmotion emotion="ANGRY" key="1021" />
  <wait duration="500" key="1022" />
  <light state="ON" color="WHITE" key="1023" />
  <evaEmotion emotion="NEUTRAL" key="1024" />
  <talk key="1025">$, ¿que te parece si jugamos un juego de imitación?</talk>
  <listen key="1026" />
  <switch var="$">
  <case op="exact" value="si" key="1027" child_proc="true" var="$">
  <evaEmotion emotion="HAPPY" key="1028" />
  <light state="ON" color="YELLOW" key="1029" />
  <talk key="1030">Muy bien, entonces trata de imitar mis emociones</talk>
  <counter var="IT" op="=" value="0" key="1031" />
  <counter var="P" op="=" value="0" key="1032" />
  <random id="R" min="1" max="3" key="1033" />
  <switch var="$">
  <case op="eq" value="1" key="1034" child_proc="true" var="$">
  <light state="ON" color="GREEN" key="1035" />
  <evaEmotion emotion="HAPPY" key="1036" />
  <userEmotion key="1037" />
  <switch var="$">
  <case op="exact" value="HAPPY" key="1038" child_proc="true" var="$">
  <counter var="P" op="+" value="1" key="1039" />
  </case>
  </switch>
  </case>
  <case op="eq" value="2" key="1040" child_proc="true" var="$">
  <light state="ON" color="BLUE" key="1041" />
  <evaEmotion emotion="SAD" key="1042" />
  <userEmotion key="1043" />
  <switch var="$">
  <case op="exact" value="SAD" key="1044" child_proc="true" var="$">
  <counter var="P" op="+" value="1" key="1045" />
  </case>
  </switch>
  </case>
  <case op="eq" value="3" key="1046" child_proc="true" var="$">
  <light state="ON" color="RED" key="1047" />
  <evaEmotion emotion="ANGRY" key="1048" />
  <userEmotion key="1049" />
  <switch var="$">
  <case op="exact" value="ANGRY" key="1050" child_proc="true" var="$">
  <counter var="P" op="+" value="1" key="1051" />
  </case>
  </switch>
  </case>
  </switch>
  <evaEmotion emotion="NEUTRAL" key="1052" />
  <light state="ON" color="WHITE" key="1053" />
  <counter var="IT" op="+" value="1" key="1054" />
  <switch var="IT">
  <case op="eq" value="3" key="1055" child_proc="true" var="IT">
  <talk key="1056">Bueno finalmente acumulaste un total de #P, ahora me despido, que tengas lindo dia $1</talk>
  </case>
  <default key="1057" child_proc="true" value="" op="exact">
  <goto target="R" />
  </default>
  </switch>
  </case>
  <case op="exact" value="no" key="1058" child_proc="true" var="$">
  <evaEmotion emotion="SAD" key="1059" />
  <light state="ON" color="BLUE" key="1060" />
  <talk key="1061">Muy bien, bueno aqui me despido, espero poder verte pronto</talk>
  </case>
  </switch>
  </script>
