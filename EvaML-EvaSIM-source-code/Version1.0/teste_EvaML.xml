<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="teste" xsi:noNamespaceSchemaLocation="EvaML-Schema/evaml_schema.xsd" id="698dc19d489c4e4db73e28a713eab07b">
  <settings>
    <voice tone="pt-BR_IsabelaV3Voice" key="1000" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <motion type="CENTER" key="1001" />
    <light state="ON" color="WHITE" key="1002" />
    <evaEmotion emotion="NEUTRAL" key="1003" />
    <audio source="efx-wand1" block="TRUE" key="1004" />
    <talk key="1005">Olá, eu sou a nova versão do robô EVA</talk>
    <wait duration="1000" key="1006" />
    <talk key="1007">Tenho um novo sistema de controle totalmente modular e feito em Páiton</talk>
    <wait duration="1000" key="1008" />
    <talk key="1009">isso permite a adição de novas funcionalidades ao meu sistema </talk>
    <wait duration="1000" key="1010" />
    <talk key="1011">todas essas funcionalidades são controladas através de mensagens MQTT</talk>
    <motion type="2LEFT" key="1012" />
    <motion type="DOWN" key="1013" />
    <wait duration="1000" key="1014" />
    <light state="ON" color="GREEN" key="1015" />
    <talk key="1016">Neste momento, estou executando um script feito na linguagem Eva ML e que está sendo executado no simulador Eva SIM</talk>
    <motion type="2RIGHT" key="1017" />
    <motion type="DOWN" key="1018" />
    <wait duration="1000" key="1019" />
    <talk key="1020">Quando falo, a luz no meu peito fica azul automaticamente</talk>
    <motion type="CENTER" key="1021" />
    <wait duration="1000" key="1022" />
    <talk key="1023">Também ganhei um novo visual no meu displey</talk>
    <wait duration="1000" key="1024" />
    <talk key="1025">Meu olhar é renderizado em uma janela usando guifs</talk>
    <wait duration="1000" key="1026" />
    <talk key="1027">Além do novo visual, também ganhei novas emoções</talk>
    <wait duration="1000" key="1028" />
    <motion type="CENTER" key="1029" />
    <talk key="1030">Quando estou neutro</talk>
    <wait duration="400" key="1031" />
    <talk key="1032">Como agora</talk>
    <wait duration="1000" key="1033" />
    <talk key="1034">meus olhos podem piscar aleatoriamente</talk>
    <wait duration="2000" key="1035" />
    <motion type="UP" key="1036" />

    <talk key="1037">Além de poder ficar feliz</talk>
    <evaEmotion emotion="HAPPY" key="1038" />
    <led animation="HAPPY" key="1039" />
    <motion type="2UP" key="1040" />
    <audio source="song-here-comes-the-sun" block="TRUE" key="1041" />

    <talk key="1042">Triste</talk>
    <motion type="2DOWN" key="1043" />
    <motion type="2DOWN" key="1044" />
    <evaEmotion emotion="SAD" key="1045" />
    <led animation="SAD" key="1046" />
    <light state="ON" color="BLUE" key="1047" />
    <audio source="song-ben" block="TRUE" key="1048" />

    <talk key="1049">e com raiva</talk>
    <motion type="NO" key="1050" />
    <evaEmotion emotion="ANGRY" key="1051" />
    <led animation="ANGRY" key="1052" />
    <light state="ON" color="RED" key="1053" />
    <audio source="song-mj-thriller2" block="TRUE" key="1054" />
    <motion type="CENTER" key="1055" />

    <talk key="1056">Também posso ficar com medo</talk>
    <evaEmotion emotion="FEAR" key="1057" />
    <led animation="SAD" key="1058" />
    <audio source="song-fear" block="TRUE" key="1059" />

    <talk key="1060">e surpreso</talk>
    <evaEmotion emotion="SURPRISE" key="1061" />
    <led animation="SURPRISE" key="1062" />
    <light state="ON" color="YELLOW" key="1063" />
    <audio source="song-surprise" block="TRUE" key="1064" />

    <led animation="STOP" key="1065" />
    <evaEmotion emotion="NEUTRAL" key="1066" />
    <motion type="2LEFT" key="1067" />
    <motion type="UP" key="1068" />
    <motion type="LEFT" key="1069" />
    <talk key="1070">Continuo podendo controlar esta lâmpada inteligente</talk>
    <light state="ON" color="WHITE" key="1071" />
    <wait duration="1200" key="1072" />
    <light state="ON" color="RED" key="1073" />
    <wait duration="1200" key="1074" />
    <light state="ON" color="BLUE" key="1075" />
    <wait duration="1200" key="1076" />
    <light state="ON" color="GREEN" key="1077" />
    <wait duration="1200" key="1078" />
    <motion type="CENTER" key="1079" />
    <talk key="1080">e como você deve ter percebido, também estou movendo minha cabeça</talk>
    <wait duration="2000" key="1081" />
    <talk key="1082">Vou tocar uma música para encerrar</talk>
    <evaEmotion emotion="HAPPY" key="1083" />
    <motion type="UP" key="1084" />
    <motion type="NO" key="1085" />
    <led animation="RAINBOW" key="1086" />
    <audio source="song-the-girl-from-ipanema" block="TRUE" key="1087" />

    <led animation="STOP" key="1088" />
    <light state="OFF" key="1089" />
    <motion type="CENTER" key="1090" />
  </script>
