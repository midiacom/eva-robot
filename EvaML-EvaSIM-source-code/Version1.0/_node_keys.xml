<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="case_2" xsi:noNamespaceSchemaLocation="EvaML-Schema/evaml_schema.xsd" id="e3dbe89a1e307d834912181da872fffd">
  <settings>
    <voice tone="pt-BR_IsabelaV3Voice" key="1000" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <random min="1" max="3" key="1001" />
    <switch var="$" id="loop">
      <case op="eq" value="1" key="1002" child_proc="false">
        <led animation="HAPPY" key="1003" />
      </case>
      <case op="eq" value="2" key="1004" child_proc="false">
        <led animation="SAD" key="1005" />
      </case>
      <case op="eq" value="3" key="1006" child_proc="false">
        <led animation="ANGRY" key="1007" />
      </case>
    <default key="1008" child_proc="false" /></switch>
    <light state="ON" color="PINK" key="1009" />
  </script>
</evaml>