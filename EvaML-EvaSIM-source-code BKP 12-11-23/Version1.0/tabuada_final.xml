                  </case>
                  <case op="exact" value="não">
                    <useMacro macro="final" />
                  </case>
                </switch>
              </case>
              <default>
                <counter var="ponto" op="=" value="0" />
            <counter var="pontos" op="=" value="3" />
            <talk>Que bom que voce chegou aqui no nível dois</talk>
            <talk>Podemos começar?</talk>
            <listen />
            <switch var="$">
              <case op="exact" value="sim">
                <talk>Então nós iremos começar agora.</talk>
                <loop var="Loop" times="3">
                  <useMacro macro="Multiplicando_2" />
                  <useMacro macro="nível_2" />
                </loop>
                <switch var="#ponto">
                  <case op="gte" value="2">
                    <counter var="ponto" op="=" value="0" />
                    <talk>Que bom que voce chegou aqui no nível três</talk>
                    <talk>Podemos começar?</talk>
                    <listen />
                    <switch var="$">
                      <case op="exact" value="sim">
                        <talk>Então vamos começar.</talk>
                        <loop var="Loop" times="3">
                          <useMacro macro="Multiplicando_2" />
                          <useMacro macro="nível_3" />
                        </loop>
                        <useMacro macro="final" />
                      </case>
                      <case op="exact" value="não">
                        <useMacro macro="final" />
                      </case>
                    </switch>
                 </case>
                </switch>
              </case>
              <case op="exact" value="não">
                <useMacro macro="final" />
              </case>
            </switch>
              </default>
            </switch>
          </default>
        </switch>
      </case>
    </switch>  
      </case>
      <case op="exact" value="não">
        <talk>Certo. Então vamos começar, mas antes preciso te fazer algumas perguntas.
        </talk>
    <useMacro macro="Multiplicando" />
    <useMacro macro="Nivelamento" />
    <useMacro macro="Multiplicando" />
    <useMacro macro="Nivelamento" />
    <useMacro macro="Multiplicando" />
    <useMacro macro="Nivelamento" />
    <switch var="#pont_min">
      <case op="lt" value="2">
        <talk>Então agora iniciaremos o nível 1, vamos testar o seu conhecimento nas tabuadas</talk>
        <counter var="pontos" op="=" value="0" />
        <useMacro macro="Multiplicando" />
        <counter var="ponto" op="=" value="0" />
       <loop var="Loop" times="3">
        <useMacro macro="Multiplicando" />
        <useMacro macro="nível_1" />
       </loop>
       <counter var="multiplicando" op="=" value="0" />
       <switch var="#ponto">
        <case op="eq" value="3">
          <counter var="ponto" op="=" value="0" />
          <talk>Que bom que voce chegou aqui no nível dois</talk>
          <talk>Podemos começar?</talk>
          <listen />
          <switch var="$">
            <case op="exact" value="sim">
              <talk>Então nós iremos começar agora.</talk>
              <loop var="Loop" times="3">
                <useMacro macro="Multiplicando_2" />
                <useMacro macro="nível_2" />
              </loop>
              <switch var="#ponto">
                <case op="gte" value="2">
                  <counter var="ponto" op="=" value="0" />
                  <talk>Que bom que voce chegou aqui no nível três</talk>
                  <talk>Podemos começar?</talk>
                  <listen />
                  <switch var="$">
                    <case op="exact" value="sim">
                      <talk>Então vamos começar.</talk>
                      <loop var="Loop" times="3">
                        <useMacro macro="Multiplicando_2" />
                        <useMacro macro="nível_3" />
                      </loop>
                      <useMacro macro="final" />
                    </case>
                    <case op="exact" value="não">
                      <useMacro macro="final" />
                    </case>
                  </switch>
               </case>
              </switch>
            </case>
            <case op="exact" value="não">
              <useMacro macro="final" />
            </case>
          </switch>
        </case>  
       </switch>
      </case>
      <case op="gte" value="2">
        <useMacro macro="Multiplicando" />
        <useMacro macro="Nivelamento_2" />
        <useMacro macro="Multiplicando" />
        <useMacro macro="Nivelamento_2" />
        <switch var="#pont_min">
          <case op="lte" value="6">
            <counter var="ponto" op="=" value="0" />
            <counter var="pontos" op="=" value="3" />
            <talk>Que bom que voce chegou aqui no nível dois</talk>
            <talk>Podemos começar?</talk>
            <listen />
            <switch var="$">
              <case op="exact" value="sim">
                <talk>Então nós iremos começar agora.</talk>
                <loop var="Loop" times="3">
                  <useMacro macro="Multiplicando_2" />
                  <useMacro macro="nível_2" />
                </loop>
                <switch var="#ponto">
                  <case op="gte" value="2">
                    <counter var="ponto" op="=" value="0" />
                    <talk>Que bom que voce chegou aqui no nível três</talk>
                    <talk>Podemos começar?</talk>
                    <listen />
                    <switch var="$">
                      <case op="exact" value="sim">
                        <talk>Então vamos começar.</talk>
                        <loop var="Loop" times="3">
                          <useMacro macro="Multiplicando_2" />
                          <useMacro macro="nível_3" />
                        </loop>
                        <useMacro macro="final" />
                      </case>
                      <case op="exact" value="não">
                        <useMacro macro="final" />
                      </case>
                    </switch>
                 </case>
                </switch>
              </case>
              <case op="exact" value="não">
                <useMacro macro="final" />
              </case>
            </switch>
          </case>
          <default>
            <useMacro macro="Multiplicando" />
            <useMacro macro="Nivelamento_3" />
            <switch var="#pont_min">
              <case op="gte" value="19">
                <counter var="pontos" op="=" value="6" />
                <counter var="ponto" op="=" value="0" />
                <talk>Que bom que voce chegou aqui no nível três</talk>
                <talk>Podemos começar?</talk>
                <listen />
                <switch var="$">
                  <case op="exact" value="sim">
