<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="script01" xsi:noNamespaceSchemaLocation="EvaML-Schema/evaml_schema.xsd" id="4bc9160a149eddcf36a458f53d9cf58a">
  <settings>
    <voice tone="pt-BR_IsabelaV3Voice" key="1000" />
  </settings>
  <script>
    <evaEmotion emotion="NEUTRAL" key="1001" />
      <light state="ON" color="WHITE" key="1002" />
      <talk key="1003">Olá eu sou o robô Eva</talk>
      <audio source="fanfare2" block="TRUE" key="1004" />
      <talk key="1005">Qual o seu nome?</talk>
      <listen key="1006" />
    <talk key="1007">Muito prazer $</talk>
    <talk key="1008">Três variáveis serão criadas. Olha na tabela ao lado </talk>
    <counter var="var1" op="=" value="0" key="1009" />
      <counter var="var2" op="=" value="0" key="1010" />
      <counter var="var3" op="=" value="0" key="1011" />
    <random min="1" max="3" key="1012" />
    <talk key="1013">Veja como estou me sentindo hoje</talk>
    <switch var="$">
      <case op="exact" value="1" key="1014" child_proc="true" var="$">
        <evaEmotion emotion="SAD" key="1015" />
        <light state="ON" color="BLUE" key="1016" />
        <talk key="1017">Estou triste</talk>
        <motion type="2DOWN" key="1018" />
        <talk key="1019">Vou incrementar a variável var 1 em uma unidade</talk>
        <counter var="var1" op="+" value="1" key="1020" />
      </case>
      <case op="exact" value="2" key="1021" child_proc="true" var="$">
        <evaEmotion emotion="ANGRY" key="1022" />
        <light state="ON" color="RED" key="1023" />
        <talk key="1024">Estou com raiva</talk>
        <motion type="2DOWN" key="1025" />
        <talk key="1026">Vou incrementar a variável var 2 em duas unidades</talk>
        <counter var="var2" op="+" value="2" key="1027" />
      </case>
      <case op="exact" value="3" key="1028" child_proc="true" var="$">
        <evaEmotion emotion="HAPPY" key="1029" />
        <light state="ON" color="GREEN" key="1030" />
        <talk key="1031">Estou muito feliz</talk>
        <motion type="2UP" key="1032" />
        <talk key="1033">Vou incrementar a variável var 3 em três unidades</talk>
        <counter var="var3" op="+" value="3" key="1034" />
      </case>
    </switch>
    <evaEmotion emotion="NEUTRAL" key="1035" />
    <talk key="1036">Ouça essa música e me diga como você está se sentindo</talk>
    <audio source="song-vivaldi-spring" block="FALSE" key="1037" />
    <listen key="1038" />
    <talk key="1039">Nossa você está $</talk>
      <talk key="1040">Até mais, $1</talk>
      <light state="OFF" key="1041" />
    </script>
