<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="game-of-emotions-english-ok" xsi:noNamespaceSchemaLocation="/home/marcelo/Dropbox/Estudo XML/EvaML-Schema/evaml_schema.xsd" id="a57ef064a897c5d104a108b4f65cbccc">
	<settings>
		<voice tone="en-US_AllisonV3Voice" key="1000" />
	</settings>
	<script>
		
		<light state="on" color="white" key="1001" />
		<evaEmotion emotion="neutral" key="1002" />
		<talk key="1003">Hi, I am the robot Eva!</talk>
		<audio source="mario-game-intro" block="true" key="1004" />
		<talk key="1005">Shall we play the emotions game?</talk>
		<evaEmotion emotion="happy" key="1006" />
		<audio source="mario-start-01" block="true" key="1007" />
		<evaEmotion emotion="neutral" key="1008" />
		<talk key="1009">What is your name?</talk>
		<audio source="mario-sound1" block="false" key="1010" />
		<listen key="1011" />
		<evaEmotion emotion="happy" key="1012" />
		<talk key="1013">Very nice!</talk>
		<talk key="1014">Let's go $</talk>
		<evaEmotion emotion="neutral" key="1015" />
		<talk key="1016">I will show you my emotions</talk>
		<audio source="mario-sound1" block="true" key="1017" />
		<talk key="1018">I look like this when I am happy</talk>
		<evaEmotion emotion="happy" key="1019" />
		<light state="on" color="green" key="1020" />
		<led animation="happy" key="1021" />
		<wait duration="2000" key="1022" />
		<evaEmotion emotion="neutral" key="1023" />
		<talk key="1024">I look like this when I am sad</talk>
		<audio source="mario-sound1" block="true" key="1025" />
		<evaEmotion emotion="sad" key="1026" />
		<light state="on" color="blue" key="1027" />
		<led animation="sad" key="1028" />
		<wait duration="2000" key="1029" />
		<evaEmotion emotion="neutral" key="1030" />
		<talk key="1031">I look like this when I am angry</talk>
		<audio source="mario-sound1" block="false" key="1032" />
		<evaEmotion emotion="angry" key="1033" />
		<light state="on" color="red" key="1034" />
		<led animation="angry" key="1035" />
		<wait duration="2000" key="1036" />
		<evaEmotion emotion="neutral" key="1037" />
		<light state="on" color="white" key="1038" />
		<talk key="1039">Let's start the game</talk>
		
		<counter var="try" op="=" value="1" key="1040" />
		<evaEmotion id="EMOCAO1" emotion="neutral" key="1041" />
		<light state="on" color="white" key="1042" />
		<talk key="1043">How am I feeling?</talk>
		<talk key="1044">Happy, sad, or, angry?</talk>
		<evaEmotion emotion="sad" key="1045" />
		<light state="on" color="blue" key="1046" />
		<audio source="mario-sound1" block="true" key="1047" />
		<listen key="1048" />
		<switch var="$">
			<case op="contain" value="sad" key="1049" child_proc="true" var="$">
				<audio source="mario-start-02" block="true" key="1050" />
				<talk key="1051">Very good! You are right!</talk>
			</case>
			<default key="1052" child_proc="true" value="" op="exact">
				<audio source="mario-sound1" block="true" key="1053" />
				<talk key="1054">I am feeling sad</talk>
				<switch var="try">
					<case op="eq" value="1" key="1055" child_proc="true" var="try">
						<counter var="try" op="+" value="1" key="1056" />
						<wait duration="2000" key="1057" />
						<talk key="1058">Let's try one more time</talk>
						<wait duration="2000" key="1059" />
						<goto target="EMOCAO1" />
					</case>
					<case op="eq" value="2" key="1060" child_proc="true" var="try">
						<wait duration="100" key="1061" />
					</case>
				</switch>
			</default>
		</switch>
		<evaEmotion emotion="happy" key="1062" />
		<wait duration="1000" key="1063" />
		
		<counter var="try" op="=" value="1" key="1064" />
		<evaEmotion id="EMOCAO2" emotion="neutral" key="1065" />
		<light state="on" color="white" key="1066" />
		<talk key="1067">How am I feeling?</talk>
		<talk key="1068">Happy, sad, or, angry?</talk>
		<evaEmotion emotion="angry" key="1069" />
		<light state="on" color="red" key="1070" />
		<audio source="mario-sound1" block="true" key="1071" />
		<listen key="1072" />
