						<counter var="try" op="+" value="1" key="1080" />
						<wait duration="2000" key="1081" />
						<talk key="1082">Let's try one more time</talk>
						<wait duration="2000" key="1083" />
						<goto target="EMOCAO2" />
					</case>
					<case op="eq" value="2" key="1084" child_proc="true" var="try">
						<wait duration="100" key="1085" />
					</case>
				</switch>
			</default>
		</switch>
		<evaEmotion emotion="happy" key="1086" />
		<wait duration="1000" key="1087" />
		
		<counter var="try" op="=" value="1" key="1088" />
		<evaEmotion id="EMOCAO3" emotion="neutral" key="1089" />
		<light state="on" color="white" key="1090" />
		<talk key="1091">How am I feeling?</talk>
		<talk key="1092">Happy, sad, or, angry?</talk>
		<evaEmotion emotion="happy" key="1093" />
		<light state="on" color="green" key="1094" />
		<audio source="mario-sound1" block="true" key="1095" />
		<listen key="1096" />
		<switch var="$">
			<case op="contain" value="happy" key="1097" child_proc="true" var="$">
				<audio source="mario-start-02" block="true" key="1098" />
				<talk key="1099">Perfect! You are correct!</talk>
			</case>
			<default key="1100" child_proc="true" value="" op="exact">
				<audio source="mario-sound1" block="true" key="1101" />
				<talk key="1102">I am feeling happy</talk>
				<switch var="try">
					<case op="eq" value="1" key="1103" child_proc="true" var="try">
						<counter var="try" op="+" value="1" key="1104" />
						<wait duration="2000" key="1105" />
						<talk key="1106">Let's try one more time</talk>
						<wait duration="2000" key="1107" />
						<goto target="EMOCAO2" />
					</case>
					<case op="eq" value="2" key="1108" child_proc="true" var="try">
						<wait duration="100" key="1109" />
					</case>
				</switch>
			</default>
		</switch>
		<evaEmotion emotion="happy" key="1110" />
		<talk key="1111">It was really nice to play with you $1</talk>
		<audio source="mario-end-02" block="true" key="1112" />
		<light state="off" key="1113" />
	</script>
