				<audio source="mario-start-02" block="true" key="1074" />
				<talk key="1075">Nice! You are right!</talk>
			</case>
			<default key="1076" child_proc="true" value="" op="exact">
				<audio source="mario-sound1" block="true" key="1077" />
				<talk key="1078">I am feeling angry</talk>
