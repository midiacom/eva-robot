	<script>
		<light state="on" color="white" key="1001" />
		<evaEmotion emotion="neutral" key="1002" />
		<talk key="1003">Hi, I am the robot Eva!</talk>
		<audio source="mario-game-intro" block="true" key="1004" />
		<talk key="1005">Shall we play the emotions game?</talk>
		<evaEmotion emotion="happy" key="1006" />
