			<led animation="listen" key="1083" />
			<wait duration="4000" key="1084" />
			<led animation="stop" key="1085" />
			<wait duration="1000" key="1086" />
		
				<audio source="mario-sound1" block="true" key="1087" />
				<talk key="1088">I am feeling happy</talk>
						<wait duration="2000" key="1089" />
						<talk key="1090">Let's try one more time</talk>
						<wait duration="2000" key="1091" />
						<evaEmotion emotion="neutral" key="1092" />
						<light state="on" color="white" key="1093" />
						<talk key="1094">How am I feeling?</talk>
						<talk key="1095">Happy, sad, or, angry?</talk>
						<evaEmotion emotion="happy" key="1096" />
						<light state="on" color="green" key="1097" />
						<audio source="mario-sound1" block="true" key="1098" />
							<led animation="listen" key="1099" />
							<wait duration="4000" key="1100" />
							<led animation="stop" key="1101" />
							<wait duration="1000" key="1102" />
							<audio source="mario-start-02" block="true" key="1103" />
							<talk key="1104">Perfect! You are correct </talk>
		<evaEmotion emotion="happy" key="1105" />
		<talk key="1106">It was really nice to play with you Marcelo</talk>
		<audio source="mario-end-02" block="true" key="1107" />
		<light state="off" key="1108" />
