<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="game-of-colors-english" xsi:noNamespaceSchemaLocation="/home/marcelo/Dropbox/Estudo XML/EvaML-Schema/evaml_schema.xsd" id="6b58971787be3197ab25f38adaef55f9">
	<settings>
		<voice tone="en-US_AllisonV3Voice" key="1000" />
	</settings>
	<script>
		<light state="off" key="1001" />
		<evaEmotion emotion="neutral" key="1002" />
		<talk key="1003">Hi, I'm the robot Eva!</talk>
		<audio source="mario-game-intro" block="true" key="1004" />
		<talk key="1005">Shall we play the color game?</talk>
		<evaEmotion emotion="happy" key="1006" />
		<audio source="mario-start-01" block="true" key="1007" />
		<talk key="1008">What's your name?</talk>
		<audio source="mario-sound1" block="true" key="1009" />
		<listen key="1010" />
		<talk key="1011">That Cool!</talk>
		<talk key="1012">Let's go $</talk>
		
		<counter var="try" op="=" value="1" key="1013" />
		<evaEmotion id="COR1" emotion="neutral" key="1014" />
		<talk key="1015">What is this color?</talk>
		<audio source="mario-sound1" block="true" key="1016" />
		<light state="on" color="blue" key="1017" />
		<listen key="1018" />
		<switch var="$">
			<case op="contain" value="blue" key="1019" child_proc="true" var="$">
				<audio source="mario-start-02" block="true" key="1020" />
				<talk key="1021">Very good! You're right!</talk>
			</case>
			<default key="1022" child_proc="true" value="" op="exact">
				<audio source="mario-sound1" block="true" key="1023" />
				<talk key="1024">This color is blue</talk>
				<switch var="try">
					<case op="eq" value="1" key="1025" child_proc="true" var="try">
						<counter var="try" op="+" value="1" key="1026" />
						<wait duration="2000" key="1027" />
						<talk key="1028">Let's try one more time</talk>
						<light state="off" key="1029" />
						<wait duration="2000" key="1030" />
						<goto target="COR1" />
					</case>
					<case op="eq" value="2" key="1031" child_proc="true" var="try">
						<wait duration="100" key="1032" />
					</case>
				</switch>
			</default>
		</switch>
		<evaEmotion emotion="happy" key="1033" />
		<light state="off" key="1034" />
		<wait duration="1000" key="1035" />
		
		<counter var="try" op="=" value="1" key="1036" />
		<evaEmotion id="COR2" emotion="neutral" key="1037" />
		<talk key="1038">What is this color?</talk>
		<audio source="mario-sound1" block="true" key="1039" />
		<light state="on" color="red" key="1040" />
		<listen key="1041" />
		<switch var="$">
			<case op="contain" value="red" key="1042" child_proc="true" var="$">
				<audio source="mario-start-02" block="true" key="1043" />
				<talk key="1044">Excelent! You're right!</talk>
			</case>
			<default key="1045" child_proc="true" value="" op="exact">
				<audio source="mario-sound1" block="true" key="1046" />
				<talk key="1047">This color is red</talk>
				<switch var="try">
					<case op="eq" value="1" key="1048" child_proc="true" var="try">
						<counter var="try" op="+" value="1" key="1049" />
						<wait duration="2000" key="1050" />
						<talk key="1051">Let's try one more time</talk>
						<light state="off" key="1052" />
						<wait duration="2000" key="1053" />
						<goto target="COR2" />
					</case>
					<case op="eq" value="2" key="1054" child_proc="true" var="try">
						<wait duration="100" key="1055" />
					</case>
				</switch>
			</default>
		</switch>
		<evaEmotion emotion="happy" key="1056" />
		<light state="off" key="1057" />
		<wait duration="1000" key="1058" />
		
		<counter var="try" op="=" value="1" key="1059" />
		<evaEmotion id="COR3" emotion="neutral" key="1060" />
		<talk key="1061">What is this color?</talk>
		<audio source="mario-sound1" block="true" key="1062" />
		<light state="on" color="green" key="1063" />
		<listen key="1064" />
		<switch var="$">
			<case op="contain" value="green" key="1065" child_proc="true" var="$">
				<audio source="mario-start-02" block="true" key="1066" />
				<talk key="1067">Nice! You're correct!</talk>
			</case>
			<default key="1068" child_proc="true" value="" op="exact">
				<audio source="mario-sound1" block="true" key="1069" />
				<talk key="1070">This color is green</talk>
				<switch var="try">
					<case op="eq" value="1" key="1071" child_proc="true" var="try">
						<counter var="try" op="+" value="1" key="1072" />
						<wait duration="2000" key="1073" />
						<talk key="1074">Let's try one more time</talk>
						<light state="off" key="1075" />
						<wait duration="2000" key="1076" />
						<goto target="COR3" />
					</case>
					<case op="eq" value="2" key="1077" child_proc="true" var="try">
						<wait duration="100" key="1078" />
					</case>
				</switch>
			</default>
		</switch>
		<evaEmotion emotion="happy" key="1079" />
		<talk key="1080">It was really nice to play with you $1</talk>
		<audio source="mario-end-02" block="true" key="1081" />
		<light state="off" key="1082" />
	</script>
